
public class Princimal {

	public static void main(String[] args) {
		// Creando objetos de la clase coche.
		// NombreClase nombreObjeto = new NombreClase(...);

		Coche cocheJoseM = new Coche("4279DHD", "Audi", "A3", "negro", 2553.75, 1900, 1.2, 'd', true, 5);
		Coche cocheMore = new Coche("2334DHD", "Mercedes", "CLA", "azul", 10553.75, 1900, 1.2, 'd', true, 5);
		Coche cocheDavid = new Coche("4234DHD", "Citroen", "saxo", "verde", 432.6, 1200, 1.1, 'd', true, 5);
		Coche cocheJC = new Coche("1234DJL", "Volvo", "S60", "Rosita", 4500.75, 2500, 1.6, 'g', false, 5);

		System.out.println(cocheJoseM.getColor());
		System.out.println(cocheDavid.getMatricula());
		System.out.println(cocheJC.getCilindrada());

		cocheDavid.setColor("azul marino");
		System.out.println(cocheJC.getColor());

		cocheJC.setCombustible('d');
		System.out.println("El combustible de Joselito Silver Feet es: " + cocheJoseM.getCombustible());
		
		System.out.println(cocheJC.toString());
		
		//MOTO
		
		Moto motoJoseM = new Moto("2341DAS", "Honda", "GLR", 125, true, 2015, true, true, 2, 6.7, 'c');
		Moto motoMore  = new Moto("2131DCD", "Kawasaki", "KX", 125, false, 2008, false, false, 2, 5.4, 't');
		Moto motoJC    = new Moto("5453DAC", "Yamaha", "X-MAX", 250, true, 2005, true, true, 2, 9, 'u');
		
		System.out.println(motoMore.getMarca());
		motoJC.setMatricula("23234VDE");
		System.out.println(motoJC.getMatricula());
		System.out.println(motoJoseM.toString());
	}

}
